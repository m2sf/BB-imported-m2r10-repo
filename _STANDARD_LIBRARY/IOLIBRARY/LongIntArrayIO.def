DEFINITION MODULE LongIntArrayIO [ProtoArrayIO] FOR LONGINT;

(* IO Extension Library for Array Types of LONGINT *)

IMPORT FileIO;

PROCEDURE [READ] ReadArray
  ( infile : FileIO.File; VAR array : BARE ARRAY OF LONGINT );

PROCEDURE [WRITE] WriteArray
  ( outfile : FileIO.File; CONST array : BARE ARRAY OF LONGINT );

PROCEDURE [WRITEF] WriteFArray
  ( outfile : FileIO.File;
    CONST fmtStr : ARRAY OF CHAR;
    CONST arrays : ARGLIST >0 OF BARE ARRAY OF LONGINT );

END LongIntArrayIO.