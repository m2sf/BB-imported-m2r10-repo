DEFINITION MODULE IOSupport;

(* Aggregator Module for Channel I/O Operations of Predefined Types *)

IMPORT StdIO+;
(* Imports StdIO into the importing modue. *)

IMPORT BooleanBaseIO+, BooleanArrayIO+;
(* Imports IO modules for predefined type BOOLEAN into the importing module. *)

IMPORT CharBaseIO+, CharArrayIO+, CharStrIO+;
(* Imports IO modules for predefined type CHAR into the importing module. *)

IMPORT UnicharBaseIO+, UnicharArrayIO+, UnicharStrIO+;
(* Imports IO modules for predefined type UNICHAR into the importing module. *)

IMPORT OctetBaseIO+, OctetArrayIO+;
(* Imports IO modules for predefined type OCTET into the importing module. *)

IMPORT CardinalBaseIO+, CardinalArrayIO+;
(* Imports IO modules for predefined type CARDINAL into the importing module. *)

IMPORT LongCardBaseIO+, LongCardArrayIO+;
(* Imports IO modules for predefined type LONGCARD into the importing module. *)

IMPORT IntegerBaseIO+, IntegerArrayIO+;
(* Imports IO modules for predefined type INTEGER into the importing module. *)

IMPORT LongIntBaseIO+, LongIntArrayIO+;
(* Imports IO modules for predefined type LONGINT into the importing module. *)

IMPORT RealBaseIO+, RealArrayIO+;
(* Imports IO modules for predefined type REAL into the importing module. *)

IMPORT LongRealBaseIO+, LongRealArrayIO+;
(* Imports IO modules for pervasive type LONGREAL into the importing module. *)

END IOSupport.