DEFINITION MODULE CardinalArrayIO [ProtoArrayIO] FOR CARDINAL;

(* IO Extension Library for Array Types of CARDINAL *)

IMPORT FileIO;

PROCEDURE [READ] ReadArray
  ( infile : FileIO.File; VAR array : BARE ARRAY OF CARDINAL );

PROCEDURE [WRITE] WriteArray
  ( outfile : FileIO.File; CONST array : BARE ARRAY OF CARDINAL );

PROCEDURE [WRITEF] WriteFArray
  ( outfile : FileIO.File;
    CONST fmtStr : ARRAY OF CHAR;
    CONST arrays : ARGLIST >0 OF BARE ARRAY OF CARDINAL );

END CardinalArrayIO.