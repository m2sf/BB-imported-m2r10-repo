DEFINITION MODULE LongCardArrayIO [ProtoArrayIO] FOR LONGCARD;

(* IO Extension Library for Array Types of LONGCARD *)

IMPORT FileIO;

PROCEDURE [READ] ReadArray
  ( infile : FileIO.File; VAR array : BARE ARRAY OF LONGCARD );

PROCEDURE [WRITE] WriteArray
  ( outfile : FileIO.File; CONST array : BARE ARRAY OF LONGCARD );

PROCEDURE [WRITEF] WriteFArray
  ( outfile : FileIO.File;
    CONST fmtStr : ARRAY OF CHAR;
    CONST arrays : ARGLIST >0 OF BARE ARRAY OF LONGCARD );

END LongCardArrayIO.