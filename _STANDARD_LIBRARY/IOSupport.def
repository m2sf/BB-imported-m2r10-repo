DEFINITION MODULE IOSupport;

(* Aggregator Module for I/O Operations of Predefined Types *)

IMPORT BooleanIO+;
(* Imports IO module for predefined type BOOLEAN into the importing module. *)

IMPORT CharIO+;
(* Imports IO module for predefined type CHAR into the importing module. *)

IMPORT UnicharIO+;
(* Imports IO module for predefined type UNICHAR into the importing module. *)

IMPORT OctetIO+;
(* Imports IO module for predefined type OCTET into the importing module. *)

IMPORT CardinalIO+;
(* Imports IO module for predefined type CARDINAL into the importing module. *)

IMPORT LongCardIO+;
(* Imports IO module for predefined type LONGCARD into the importing module. *)

IMPORT IntegerIO+;
(* Imports IO module for predefined type INTEGER into the importing module. *)

IMPORT LongIntIO+;
(* Imports IO module for predefined type LONGINT into the importing module. *)

IMPORT RealIO+;
(* Imports IO module for predefined type REAL into the importing module. *)

IMPORT LongRealIO+;
(* Imports IO module for pervasive type LONGREAL into the importing module. *)

END IOSupport.