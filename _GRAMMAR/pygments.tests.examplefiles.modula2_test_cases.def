(*- Math Symbol Substitution -*)

(* the following operators are replaced with proper math symbols *)

(* not-equal operator # *)

IF foo # bar THEN ... END;

(* not-less operator >= *)

IF foo >= bar THEN ... END;

(* not-greater operator <= *)

IF foo <= bar THEN ... END;

(* identity operator == *)

IF foo == bar THEN ... END;

(* dot product operator *. *)

dotProduct := v1 *. v2;

(* NOT operator synonym ~ *)

not := ~ a;


