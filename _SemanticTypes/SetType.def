BINDINGS FOR SetType;

(* Semantic Properties for Static Set Types *)


(* ADTs must be opaque records,
   structured literals may be used *)

TYPE = OPAQUE RECORD := {};


(* Bindings required for SetType ADTs *)

PROCEDURE [:=]; (* require binding to assignment operator *)

PROCEDURE [::]; (* optional bindings to conversion operator *)

PROCEDURE [?]; (* required accessor binding to [ ] operator *)

PROCEDURE [!]; (* require storage mutator binding to [ ] operator *)

PROCEDURE [COUNT]; (* require binding to COUNT function *)

PROCEDURE [FOR]; (* require binding to FOR .. IN iterator *)

PROCEDURE [+]; (* require binding to set union operator *)

PROCEDURE [-]; (* require binding to set difference operator *)

PROCEDURE [*]; (* require binding to set intersection operator *)

PROCEDURE [/]; (* require binding to symmetric set difference op *)

PROCEDURE [=]; (* require binding to equal operator *)

PROCEDURE [<]; (* require binding to subset operator *)

PROCEDURE [>]; (* alrequirelow binding to superset operator *)

END SetType.