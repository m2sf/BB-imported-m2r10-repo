DEFINITION MODULE LONGLONGBITSET;

(* Alias type for 128-bit bitset type *) 

<* IF TSIZE(LONGBITSET)*8 = 128 *>

TYPE LONGLONGBITSET = ALIAS OF LONGBITSET;

<* ELSE *>

IMPORT BS128;

TYPE LONGLONGBITSET = ALIAS OF BS128;

<* ENDIF *>

END LONGLONGBITSET.